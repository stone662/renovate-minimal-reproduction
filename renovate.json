{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "regexManagers": [
        {
            "fileMatch": ["base_images.txt"],
            "matchStrings": [
                "datasource=(?<datasource>\\S+) depName=(?<depName>\\S+)( versioning=(?<versioning>\\S+))?\n.*?-[^:]*:(?<currentValue> ?\\d.*)"
            ]
        }
    ]
}